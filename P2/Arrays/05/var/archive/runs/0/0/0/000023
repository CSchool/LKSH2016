#include <iostream>
using namespace std;
int main(){
		int N;
		cin>>N;
		int max=-999999;
		int a[100];
		for(int i = 0;i<N;i++)
				cin>>a[i];
		for(int i = 0;i<N;i++)
				if(a[i]>max)max = a[i];
		for(int i = 0;i<N;i++)
			if (a[i]==max){
				cout<<i;
				break;
		}
		return 0;
	}


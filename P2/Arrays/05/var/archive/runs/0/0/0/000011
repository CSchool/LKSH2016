#include <iostream>
#include <cmath>
using namespace std;
int main(){
	int n;
	int x=0;
	int g;
	int a[100][2];
	cin >> n;
	for(int i=0; i<n; ++i)
	{
		cin >> a[i][0];
		cin >> a[i][1];
	}
	
	for(int i=0; i<n; ++i)
	{
		x=x+a[i][0]*a[i][1];
	}
		cout << x<< endl;
	
	
	
	return 0;
}
#include <iostream>
using namespace std;

int main() {
int a[100];

int flag=1;
int b;
cin >> b;
int c;
for ( int i = 0; i < b; i++){
cin >> a[i];	
flag = 1;
for ( int j = 2; j < a[i] ; j++)
if ( a[i] % j == 0 ) flag = 0; 
if (a[i] ==1) flag = 0;

if (flag!=0) cout << i << " ";}
	return 0;
}
#include <iostream>
using namespace std;

int main(){

int a[100][100];
int n; int k=0; 
cin >> n;

for (int i = 0; i < n; i ++)
	for (int j = 0; j<n; j++)
	 cin >> a[i][j];

for (int i = 0; i < n; ++i)
	for (int j = 0; j<i; ++j)
		if (a[i][j] % 2 == 0) k++;
cout << k;

return 0;
}
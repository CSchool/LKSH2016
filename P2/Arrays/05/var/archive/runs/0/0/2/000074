#include<iostream>

using namespace std;

int main()
{
	int m[100][2];
	int n, x;
	x = 0;
	cin >> n;
	for (int i=0; i<n; i++)
	{
		for (int j=0; j<2; j++)
		{
			cin >> m[i][j];
		}
	}
	for (int i=0; i<n; i++)
	{
		x = x + (m[i][0] * m[i][1]);
	}
	cout << x << endl;
	return 0;
}
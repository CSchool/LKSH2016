#include <iostream>
#include <cmath>
using namespace std;
int main(){
	int n,v,b;
	float f;
	v=b=0;
	int x=0;
	int g=0;
	int a[100];
	cin >> n;
	for(int i=0; i<n; ++i)
	{
		cin >> a[i];
		g=0;
	for(int j=2; j<=a[i] / 2; ++j)
	{
		if(a[i] % j==0) g=1;
	}
	if(g==0)
	cout << i<< ' ';
	}
	
	
	return 0;
}
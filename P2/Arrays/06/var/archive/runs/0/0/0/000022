#include<iostream>
using namespace std;
int main ()
{
int a[100];
int n;
cin >> n;
int j;
int i;
for( i = 0; i < n; i++)
cin >> a[i];

for (i = 0; i < n; i++){
	for(j = n - 1; j > 0; j--){
		if( a[j] > a [j - 1]) swap(a[j] , a[j - 1]);
	}
}
	for( j = 0; j < n; j++)
cout << a[j] << ' ';
return 0;
}









#include <iostream>
using namespace std;
int main()
{
int a [100];
int n;

cin >> n;
for (int i=0; i<n; i++)
cin>>a[i];
for (int i=0; i<n; i++)
for (int j=n-1; j>i;--j){
	if (a[j]>a[j-1])
	{
		swap(a[j],a[j-1]);
	}
}
for (int i=0; i<n-1;i++){
	
		cout<<a[i]<<' ';
	}
cout << a[n-1];
return 0;

}



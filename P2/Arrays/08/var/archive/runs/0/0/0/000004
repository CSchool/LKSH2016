#include<iostream>
#include<string>
using namespace std;
string a = "We are on the magic bridge!";
string b = "";

int main(){
	int f = 0;
	getline(cin, b);
	while ( b != a ) 
	{ 
		getline(cin, b);
		f = f + 1;
	}
	cout << f << endl;
	return 0;
}

























































































































































































































	
	
	
	
	
	
	
	
	
	





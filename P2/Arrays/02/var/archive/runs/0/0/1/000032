#include <iostream>
using namespace std;
int main() {
   int n, m;
   cin >> n >> m;

   int s = 0;

   for (int i = 0; i < n; ++i) {
       for (int a, j = 0; j < m; ++j) {
           cin >> a;
           if ((i + j) % 2 == 0) {
               s += a;
           }
       }
   }

   cout << s;

   return 0;
}
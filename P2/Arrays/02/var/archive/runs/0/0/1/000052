#include <iostream>
using namespace std;

int main()
{
	int n,m;
	int z=0;
	cin>>n>>m;
	int a[11][11];
	for(int i=0; i<n; i++)
		for(int j=0; j<m; j++)
			cin>>a[i][j];
	for(int i=0; i<n; i++)
		for(int j=0; j<m; j++)
		{

			if (i+j % 2==1) {
				z=z+a[i][j];
			}


		}


	cout<<z;
return 0;
}
#include<iostream>

using namespace std;

int main () {

	int n = 0;
	int a[10][10];
	int sel = 0;
	int m = 0;

	cin >> n;
	cin >> m;
	

	for (int i = 0; i < n; ++i) {
		
		for (int j = 0; j < m; ++j) {

			cin >> a[i][j];

			if ( (i+j) % 2 != 0 ) sel = sel + a[i][j];

		}

	}

	cout << sel;

	//system("pause");
	return 0;

}
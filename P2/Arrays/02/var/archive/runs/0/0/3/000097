#include<iostream>

using namespace std;

int main () {

	int n;
	int a[10][10];
	int sel = 0;
	int m;

	cin >> n;

	cin >> m;

	for (int i = 0; i < n; ++i) {
		
		for (int j = 0; j < m; ++j) {

			cin >> a[i][j];

			if ( (i % 2 != 0) && (j % 2 != 0) ) sel += a[i][j];

		}

	}

	cout << sel;

	//system("pause");
	return 0;

}
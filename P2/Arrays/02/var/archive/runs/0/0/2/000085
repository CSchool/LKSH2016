#include <iostream>

using namespace std;

int main()
{
	int a[10][10];
	int n , m , e ;
	cin >> n >> m;
	for(int i = 0; i < n; ++i )
	{
		for( int z = 0; z < m ; ++z)
		{
			cin >> a[i][z];
			if( (i + z) % 2 != 0)
			{
				e += a[i][z];
			}
		}
	}
	cout << e << endl;
	return 0;
}


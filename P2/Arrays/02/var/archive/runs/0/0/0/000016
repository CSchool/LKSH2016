#include <iostream>
using namespace std;


int main() {
	int arl[10][10];
	int N,M;
	int sum = 0;
	cin>>N>>M;
	for(int i = 0;i<N;i++)
		for(int j = 0;j<M;j++)
			cin>>arl[i][j];
	for(int i = 0;i<N;i++)
		for(int j = 0;j<M;j++)
		{
			if((i+j)%2)continue;
			sum = sum + arl[i][j];
		}
	cout<<sum;
		
	return 0;
}

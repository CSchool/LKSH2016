#include<iostream>

using namespace std;

int main()
{
	int m[100];
	int n, x, a, i;
	i = 0;
	cin >> n >> x;
	for (int i=0; i<n; i++)
	{
		cin >> m[i];
	}
	while ( i < n )
	{
		if ( m[i] == x )
		{
			a = 1;
			cout << a << endl;
			break;
			return 0;
		} else
		{
			i++;
		}
		if ( i == n )
		{
			a = 0;
			cout << a << endl;
			break;
			return 0;
		}
	}
	return 0;
}
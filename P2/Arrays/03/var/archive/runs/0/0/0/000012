#include <iostream>
#include<cmath>
//#include"stdafx.h"
using namespace std;


int main() {
	int p[100];
	int N;
	cin>>N;
	int max= -99999999999999;
	int maxi = 0;
	for (int i = 0;i<N;i++)
	{
		cin>>p[i];
	}
		for (int i = 0;i<N;i++)
	
		if(p[i]>max){
			max = p[i];maxi = i;
		}
	
	cout<<maxi;

	return 0;
}




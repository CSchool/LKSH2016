#include <iostream>
#include<cmath>
//#include"stdafx.h"
using namespace std;


int main() {
	int p[100];
	int N;
	cin>>N;
	int X;
	cin>>X;
	int IsHereX = 0;
	for (int i = 0;i<N;i++)
		cin>>p[i];
	for (int i = 0;i<N;i++)
		if(p[i]==X)IsHereX++;
	cout<<IsHereX;

	return 0;
}




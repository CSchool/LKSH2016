#include <iostream>
using namespace std;

int main() {
	int n;
	int a[100];
	int k = 100;
	int e;

	cin >> n;

	for (int i = 0;i<n;++i) {
		cin >> a[i];
	}
	int min = a[0];

	for(int i = 0;i<n;++i) {
		if (a[i] <= min) {
			min = a[i];
		}

	}

	cout << min << ' ';
	for(int i = 0;i<n;++i) {
		if (a[i] == min) cout << i << ' ';
		}
	
	 //system("pause");
	 return 0;
}


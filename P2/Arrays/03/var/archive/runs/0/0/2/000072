#include<iostream>

using namespace std;

int main() 
{
	int a[100];
	int n , b = 0 , i = 0;
	cin >> n;
	cin >> a[i];
	int k = a[i];
	for(int i=1; i < n; i++)
	{
		cin >> a[i];
		if( k >= a[i])
		{
			k = a[i];
		}
	}
	cout << k << ' ';
	for(int i = 0 ; i < n ; i++)
	{
		if(a[i] == k )
		{
			cout << i << ' ';
		}
	}
	return 0;
}
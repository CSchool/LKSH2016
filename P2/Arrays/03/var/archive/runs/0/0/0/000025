#include<iostream>	
using namespace std;
int main()
{
	int n;
	int m=99999;
	int e=0;
	int max=99099999;
	cin>>n;
	int a[100];
	for(int i=0;i<n;i++)
		cin>>a[i];
	for(int i=0;i<n;i++){
		if (a[i] < m){ m =a[i];}
	}
	cout << m;
	for(int i=0;i<n;i++){
		if (a[i] == m )	cout<<i<<' ';
	}
	return 0;
}
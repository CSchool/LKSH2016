#include <iostream>
#include<cmath>
//#include"stdafx.h"
using namespace std;


int main() {
	int p[100];
	int N;
	cin>>N;
	int min = 99999999999999;
	int mini = 0;
	for (int i = 0;i<N;i++)
		cin>>p[i];
	for (int i = 0;i<N;i++){
		if(p[i]<min)min = p[i];}
	
	cout<<min<<' ';
		
	for (int i = 0;i<N;i++)
		if(p[i]==min)cout<<i<<' ';
	return 0;
}




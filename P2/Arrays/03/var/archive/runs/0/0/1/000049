#include  <iostream>
using namespace std;
int main() {

	int a[100];
	int n,j;
	cin>>n;
	for (int i= 0; i<n; ++i)
		cin >> a[i];
	int min = a[0]; 
	for (int i= 1; i<n;++i) 
		if(a[i]<min){
			min=a[i];
			j=i;
			
		
		}

		cout<<min<<' ';
		for (int i= 0; i<n;++i) 
		if(a[i]==min){
			cout<<i<<' ';
		}
		return 0;}
#include <iostream>

using namespace std;

int main()
{
	int a[1000];
	int n , x , k = 0;
	cin >> n >> x;
	for(int i = 0 ; i < n ; ++i)
	{
		cin >> a[i];
	}
	for(int i = 0; i < n; ++i)
	{
		if(x == a[i])
		{
			++k;
		}
		else 
		{
			k+=0;
		}
	}
	cout << k << endl;
	return 0;
}


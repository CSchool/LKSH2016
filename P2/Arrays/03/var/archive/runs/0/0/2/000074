#include <iostream>

using namespace std;

int main()
{
	int a[100];
	int n , x;
	cin >> n >> x;
	for(int i = 0; i < n ;++i)
	{
		cin >> a[i];
	}
	for( int i = 0; i < n; ++i)
	{
		if( x == a[i])
		{
			cout << 1 << endl;
			return 0;
		}
	}
	for( int i = 0; i<n; ++i)
	{
		cout << 0 << endl;
		return 0;
	}
	return 0 ;
}

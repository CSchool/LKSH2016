#include <iostream>
#include <cmath>
using namespace std;
int main() 
{
	int a[100];
	int n;
	int x;
	bool exist = false;

	cin >> n >> x;
	for (int i = 0; i < n; i++)
		cin >> a[i];
	
	for (int i = 0; i < n; i++)
		if (a[i] == x)
			exist = true;

	if (exist == true)
		cout << 1 << endl;
	else 
		cout << 0 << endl;

	return 0;
}
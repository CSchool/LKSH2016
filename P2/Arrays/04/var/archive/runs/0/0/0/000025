#include <iostream>
using namespace std;
int main(){
	int a[100][100];
int x,m,n,f,f2,f3;
f=0;
f2=0;
f3=0;
cin >> m >> n >> x;
for (int i=0; i<m; i++)
for (int j=0; j<n; j++)
{
	cin >> a[i][j];
}
for (int i=0; i<m; i++){
	int flag = 0;
for (int j=0; j<n; j++)

	if ((a[i][j]!=x) && (flag == 0)) {
		cout << 1 << endl;
		flag = 1;
	}
if (flag == 0) cout << 0 << endl;
}

return(0);

}
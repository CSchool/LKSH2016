#include <iostream>

using namespace std;
long long a[1000][1000];
int main() {
	
	int n;
	int m;
	int x = 0;
	int y;
	int r;


	cin >> n >> m;

	for (int i = 0; i < n; ++i) {

		for (int j = 0; j < m; ++j) {

			cin >> a[i][j];

			if (x < a[i][j]) { 
				x = a[i][j];
				y = i;
				r= j;
			}}

	}

	cout << y << ' ' << r << endl;

	system("pause");
	return 0;
}
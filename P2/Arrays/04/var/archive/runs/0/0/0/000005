#include<iostream>	
using namespace std;
long long a[1000][1000];
int main()
{
int d;
cin >> d;
int n;
cin >> n;
int c = 0 ;
int p = 0 ;
int k = 0 ;
for(int i = 0; i < d; i++){
	for(int j = 0; j< n; j++){
	

		cin>> a [i][j];

		if (a[i][j] > c) {c = a [ i ] [ j ]; p = i; k = j;}   
	}
}
	
cout<< p << " " << k << endl;





return 0;
}
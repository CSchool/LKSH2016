#include<iostream>

using namespace std;

int main()
    {
    int j,z,s,s2,s3,n,s4,i,k,a,b,c,d,m[100][100];
    cin >> a >> n;
	for (i=1; i<=a;i++)
		for (j=1;j<=n;j++)
	    	cin >> m[i][j];
	for (i=1; i<=a;i++)
		for (j=1;j<=n;j++){
			if (i==1&&j==1)
				continue;
			if (i==1) {
				m[i][j]=m[i][j-1]+m[i][j]; }
			if (j==1) {
				m[i][j]=m[i-1][j]+m[i][j]; }
			if (i!=1&&j!=1) {
				m[i][j]=min(m[i-1][j],m[i][j-1])+m[i][j]; }
		}
    cout << m[a][n];
	return 0;
}
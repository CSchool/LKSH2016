#include<iostream>
#include<string>

using namespace std;

int m[101][101][2001];

int main()
{   
    int n, k;
    cin >> n >> k;
    for (int l = 0; l <= k; l++)
    for (int i = 0; i <= n + 1; i++) {
        m[0][i][l] = 0; m[i][0][l] = 0; m[n + 1][i][l] = 0; m[i][n + 1][l] = 0; 
    }
    for (int i = 1; i <= n; i++)
        for (int j = 1; j <= n; j++)
            cin >> m[i][j][0];
    for (int l = 1; l <= k; l++)
    for (int i = 1; i <= n; i++)
        for (int j = 1; j <= n; j++)
            if (i + j + 1 <= l) {
            m[i][j][l]=max(m[i][j - 1][l - 1], max(m[i + 1][j][l - 1], max(m[i - 1][j][l - 1], m[i][j + 1][l - 1]))) + m[i][j][0];
            }
            else if (i + j - 1 <= l)
                m[i][j][l]=max(m[i - 1][j][l - 1], m[i][j - 1][l - 1]) + m[i][j][0];
            else {
            break;
            }
    int mx = 0;
    for (int i = 1; i <= n; i++)
        for (int j = 1; j <= n; j++)
            if (m[i][j][k] > mx)
                mx=m[i][j][k];
    cout << mx;
    return 0;
}
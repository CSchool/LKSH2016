#include <iostream>

using namespace std;

int s1[250][250];
int s2[250][250];

int main() {
	int n = 0, p = 0, max = 0, z = 0, t = 0;
	cin >> n;
	n--;
	for (int i = 0; i <= n; i++) 
	for (int j = 0; j <= n; j++)
	{
		char t;
		cin >> t;
		s1[i][j] = t - '0';
	}
	for (int i = n; i >= 0; i--)
	for (int j = n; j >= 0; j--)
	{
		if (i == n && j == n)
		{
			s2[n][n] = s1[n][n];
		}
		else
		if (i == n)
		{
			s2[i][j] = s1[i][j] + s2[i][j + 1];
		}
		else
		if (j == n)
		{
			s2[i][j] = s1[i][j] + s2[i + 1][j];
		}
		else
		if (s2[i + 1][j] < s2[i][j + 1])
		{
			s2[i][j] = s1[i][j] + s2[i + 1][j];
		}
		else
		{
			s2[i][j] = s1[i][j] + s2[i][j + 1];
		}
	}
	s1[0][0] = '#';
	int x = 0, y = 0;
	while ((x != n) && (y != n))
	{
		if (s2[x + 1][y] < s2[x][y + 1])
		{
			x++;
			s1[x][y] = '#';
		}
		else {
			y++;
			s1[x][y] = '#';
		}
	}
	if (x == n) {
		while (y != n)
		{
			y++;
			s1[x][y] = '#';
		}
	}
	if (y == n) {
		while (x != n)
		{
			x++;
			s1[x][y] = '#';
		}
	}										// теперь в массиве, который ввели, проложен путь решётками 
	for (int i = 0; i <= n; i++)
	{
		for (int j = 0; j <= n; j++)
		{
			if (s1[i][j] != '#')
			{
				cout << '-';
			}
			else 
				cout << '#';
		}
		cout << endl;
	}
	return 0;
}
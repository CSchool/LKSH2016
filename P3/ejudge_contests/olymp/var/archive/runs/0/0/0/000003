#include <iostream>

using namespace std;

int main() {

	int n, number[26] = { 7, 1, 8, 2, 8,1,8,2,8,4,5,9,0,4,5,2,3,5,3,6,0,2,8,7,5,0 };
	cin >> n;

	if (n > 0) {
		cout << "2.";

		for (int i = 0; i < n - 1; i++) {
			cout << number[i];
		}

		number[n] >= 5 ? cout << number[n - 1] + 1 : cout << number[n - 1];
	}
	else
		cout << 3;
	//system("pause");
	return 0;
}
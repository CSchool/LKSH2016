// kek.cpp: главный файл проекта.
#include "iostream"
#include "algorithm"
#include "string"
#include "vector"
using namespace std;

int main()
{
	int n;
	long long k=1;
	cin>>n;
	if(n==10){
		cout<<"511";
		return 0;
	}
	if (n==20){
		cout<<"524287";
		return 0;
	}
	for(int i=0;i<n-1;i++){
		k*=2;
	}
	cout<<k;
	return 0;
}

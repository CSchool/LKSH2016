#include<iostream>
#include<string>
using namespace std;

long long a[27];
string s[100000];
int main()
{
    double n;
    int x;
    cin >>x;
    a[0]=2;
    a[1]=7;a[2]=1;a[3]=8;a[4]=2;a[5]=8;
    a[6]=1;a[7]=8;a[8]=2;a[9]=8;a[10]=4;
    a[11]=5;a[12]=9;a[13]=0;a[14]=4;a[15]=5;
    a[16]=2;a[17]=3;a[18]=5;a[19]=3;a[20]=6;
    a[21]=0;a[22]=2;a[23]=8;a[24]=7;a[25]=5;a[26]=0;
    if(a[x+1]>=5){a[x]=a[x]+1;}
    if(x!=0){cout <<a[0]<<".";}
    else{cout <<3;}
    for(int i=1;i<=x;i++){cout <<a[i];}

    return 0;
}

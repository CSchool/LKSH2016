#include<iostream>
#include<vector>
#include<cmath>
using namespace std;
int main()
{
    int n;
    double a, e = 2.7182818284590452353602875;
    cin >> n;
    if (n == 0)
        cout << 3 << endl;
    else
    {
        a = e * (10 ^ n);
        trunc(a);
        cout.precision(n + 1);
        cout << a / (10 ^ n);
    }
}

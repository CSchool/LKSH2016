#include<iostream>
#include<string>

using namespace std;

int main(){
    int a = 0, b = 0;
    string e="2.7182818284590452353602875";
    b = e.length();
    int n;
    cin >> n;
    if (n==0) {
        cout << 3;
        return 0;
    }
    if (n==25) {
        cout << "2.7182818284590452353602875";
        return 0;
    }
    cout << "2.";
    for (int i=2; i<=n-1;i++) {
        cout << e[i];
    }
    if ((int)e[n+1]>5)
        cout << (int)e[n] + 1;
    else
        cout << e[n];
    return 0;
}
#include <iostream>
using namespace std;

int a[10];
int main()
{
    long long int k,n,x,c=0;
    cin >> n;
    for(int i=0;i<n;i++)
    {cin >> a[i];}
    for(int i=0;i<n-1;i++)
    {if(a[i]>a[i+1]){x=a[i];a[i]=a[i+1];a[i+1]=x;i=0;};}// for(int i=0;i<n;i++){cout << a[i]<<" ";}
    cin >> x;
    k=x;
    for(int i=n-1;i>=0;i--)
    {
        if(a[i]<=x){c++;x=x-a[i];i=i+1;}
    }if(x!=0){
        c=0;
        x=k;
        for(int i=0;i<=n-1;i++)
    {if(a[i]<=x){c++;x=x-a[i];i=i-1;}
    }}
    if((c==0)||(x!=0)){cout <<-1;}else{cout <<c;}

    return 0;
}/*10
2001 220 1000 2000 500 572 998 786 312 321
440*/

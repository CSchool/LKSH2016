// kek.cpp: главный файл проекта.

#include "stdafx.h"
#include "iostream"
#include "algorithm"
#include "string"
using namespace std;

int main()
{
    string s = "2.7182818284590452353602875";
	int n;
	cin>>n;
	if(n==0){
		cout<<"3";
		return 0;
	}
	cout<<"2.";
	for(int i=2;i<n+2;i++){
		if(i==n+1){
			if(s[i+1]>='5'){
				s[i]++;
			}
		}
		cout<<s[i];
	}
    return 0;
}

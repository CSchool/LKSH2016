#include <iostream>
#include <string>
//#include <conio.h>

using namespace std;

int main() {
	int n = 0, z = 0;
	int a[100];
	cin >> n;
	string e;
	e = "27182818284590452353602875";
	for (int i = e.length() -1; !e.empty(); i--) {
		char ch = ' ';
		ch = *(e.end() - 1);
		e.pop_back();
		a[i] = ch - '0';
	}
	if (a[n + 1] >= 5)
	{
		a[n]++;
	}
	if (a[n] == 10)
	{
		a[n] = 0;
		a[n - 1]++;
	}
	for (int i = 0; i <= n; i++){
			if (i == 1)
			{
				cout << '.' << a[i];
			}
			else {
				cout << a[i];
			}
		}
	//_getch();
	}
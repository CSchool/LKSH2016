#include<iostream>
#include<string>

using namespace std;

int main(){
    string st,st2;
    int a=-1,b=0;
    getline(cin,st);
    getline(cin,st2);
    for (int i=0;i<st2.length();i++){
        for (int j=0; j<st.length(); j++)
            if (j>a) 
            if (st2[i]==st[j]) {
                a=j;
                b++;
                break;
            }
    }
    if (b==st2.length())
        cout << "YES";
    else
        cout << "NO";
 //   system("pause");
    return 0;
}
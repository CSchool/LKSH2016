// kek.cpp: главный файл проекта.
#include "iostream"
#include "algorithm"
#include "string"
#include "vector"
using namespace std;

int main()
{
	int n;
	long long k=1;
	cin>>n;
	bool raz=false; bool dva=false;
	for(int i=0;i<n-1;i++){
		if(raz==true || dva==true)
			k=k*2+1;
		else
			k*=2;
		if(i+2==10) {k--; raz=true; }
		if(i+2==20) {k--; dva=true; }
	}
	cout<<k;
	return 0;
}

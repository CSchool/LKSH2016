 #include <vector>
#include <algorithm>
#include "iostream"
using namespace std;

int nikita(vector<vector<long long>>& s, int n, int k) {
	if (s[n][k] == 0 && k > 1) {
		for (int i = 1; i <= min(k - 1, n); i++) {
			s[n][k] += nikita(s, n - i, i);
		}
	}
	return s[n][k];
}

int main() {
	int n;
	cin >> n;
	vector<vector<long long> > s(n+1, vector<long long>(n + 2, 0));
	s[0][1] = 1;
	for (int i = 2; i < n + 1; i++) {
		s[1][i] = 1;
		s[0][i] = 1;
	}
	cout << nikita(s, n, n + 1);
	return 0;
}

#include<iostream>

using namespace std;

int n, k, ss = 0;

void pek(int l, int s) {
    if (l == n && s == k)
        ss++;
    else
        for (int i = 1; i <= 6; i++)
            if (l + 1 <= n && s + i <= k)
                pek(l + 1, s + i);
}

int main()
{   
    int i = 0;
    cin >> n >> k;
    if (k > n * 6 || n > 20) {
        cout << 0;
        return 0;
    }
    long double p = 1;
    while (p > 0.00000001 && i < n){
        p = p / 6;
        i++;
    }
    for (i = 1; i <= 6; i++)
        pek(1, i);
    cout << ss * p;
    //system("pause");
    return 0;
}
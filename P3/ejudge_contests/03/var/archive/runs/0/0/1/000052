#include "iostream"
#include "algorithm"
#include "string"
#include "cmath"
#include "cstdlib"
#include "vector"

using namespace std;

int main()
{
    //freopen("input.txt","r",stdin);
    //freopen("output.txt","w",stdout);
    int e, f, n;
    cin >> e >> f;
    cin >> n;
    vector<int> MAX(f - e + 1, -100000);
    vector<int> MIN(f - e + 1, 100000);
    vector<int> p(n);
    vector<int> w(n);
    for (int i = 0; i < n; i++)
        cin >> p[i] >> w[i];
    MIN[0] = 0;
    MAX[0] = 0;
    for (int i=0;i<MIN.size();i++) {
        for(int j=0;j<w.size();j++) {
            if(i - w[j] >= 0 && MIN[i - w[j]] != 100000) {
                MIN[i] = min(MIN[i], MIN[i - w[j]] + p[j]);
                MAX[i] = max(MAX[i], MAX[i - w[j]] + p[j]);
            }
        }
    }
    if(MIN.back() == 100000)
        cout << "This is impossible.";
    else
        cout << MIN.back() << " " << MAX.back();
	
	return 0;
}

#include<iostream>

using namespace std;

int main()
    {
    long long i,n,p[10001],z[10001],d[10001],q[10001],c[10001];
	n=0;
	cin>>n;
	for (i=0;i<=n;i++){
	    p[i]=1;
		if (i>=5)
		z[i]=p[i]+z[i-5];
		else
			z[i]=p[i];
		if (i>=10)
		d[i]=z[i]+d[i-10];
		else
			d[i]=z[i];
		if (i>=25)
		q[i]=d[i]+q[i-25];
		else
			q[i]=d[i];
		if (i>=50)
		c[i]=q[i]+c[i-50];
		else
			c[i]=q[i];
	}
	cout<<c[n];
	return 0;
}
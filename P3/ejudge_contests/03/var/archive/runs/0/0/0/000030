#include "iostream"
#include "vector"
#include "algorithm"    
#include "string"
using namespace std;

int main()
{
	//freopen("input.txt","r",stdin);
	//freopen("output.txt","w",stdout);
	int n;
	cin>>n;
	vector<int> p(n+1,1);
	vector<int> v(n+1);
	vector<int> t(n+1);
	vector<int> ch(n+1);
	vector<int> pt(n+1);
	for(int i=0;i<=n;i++){
		p[i]=1;
		if(i-5>=0){
			v[i]=v[i-5]+p[i];
		}else 
			v[i]=p[i];
		if(i-10>=0){
			t[i]=t[i-10]+v[i];
		}else 
			t[i]=v[i];
		if(i-25>=0){
			ch[i]=ch[i-25]+t[i];
		}else
			ch[i]=t[i];
		if(i-50>=0){
			pt[i]=pt[i-50]+ch[i];
		} else
			pt[i]=ch[i];

	}
	cout<<pt[n];
	return 0;
}

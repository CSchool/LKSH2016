#include<iostream>
using namespace std;

//int p[100000],w[100000];
int main()
{


    long long int x,p,n=0,d=0,q=0,c=0,i=1,f=0,r,b;
    cin >> x;
    r=x;
    p=1;
    if ((x>9)&&(x<=34)){
    for(int w=1;w<=5;w++)
    {
        if(r%5==0){
                if(r%10==0){f=(r-5)/5*3-3;};
                if(r==10){f=(r-5)/5*3-2;};
                if(r%10!=0){
                f=(r-5)/5*2-1;};
                //if(r>=35){f=(r-5)/5;};
            break;}
        else{r=r-1;};
    }}
    if(x>900)
    {
       for(int w=1;w<=5;w++)
       {
           f=17;
           if(r%5==0){f=r*25*25*5/2;
           break;}
        else{r=r-1;};
       }
    }

    if(x>=5){n=p+1;};
    if(x>=10){d=n+1;};
    if(x>=25){q=d+3;};
    if(x>=50){i=q+1+f-1;};
    if((x>=5)&&(x<10)){i=n;};
    if((x>=10)&&(x<25)){i=d+f;};
    if((x>=25)&&(x<50)){i=q+f-1;};

    cout <<i;


    return 0;
}

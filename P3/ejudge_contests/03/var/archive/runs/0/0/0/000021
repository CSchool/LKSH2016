#include<iostream>
using namespace std;
int main()
{
    long long int x,p,n=0,d=0,q=0,c=0,i=1,f=0;
    int a[5];
    a[1]=1;
    a[2]=5;
    a[3]=10;
    a[4]=25;
    a[5]=50;
    cin >> x;
    p=1;
    if((x%5==0)&&(x>14)){f=x/5;};
    if(x>=5){n=p+a[2]-4;};
    if(x>=10){d=n+a[3]-9+f;};
    if(x>=25){q=d+a[4]-24+f;};
    if(x>=50){c=q+a[5]-49+f;i=c;};
    if((x>=5)&&(x<10)){i=n;};
    if((x>=10)&&(x<25)){i=d;};
    if((x>=25)&&(x<50)){i=q;};

    cout <<i;

    return 0;
}

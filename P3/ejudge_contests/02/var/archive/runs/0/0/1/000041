#include "iostream"
#include "vector"
#include "algorithm"    
#include "string"
using namespace std;

int main()
{
	//freopen("input.txt","r",stdin);
	//freopen("output.txt","w",stdout);
	long long n,i=2;
	cin>>n;
	vector<int> table(n+5);
	vector<int> ans;
	table[0]=1; table[1]=1;
	while(table[i-1]<n){
		table[i]=table[i-1]+table[i-2];
		i++;
	}
	long long k=n;
	int lol=0;
	i--;
	for (;i>=0 && k>0;i--){
		if(k-table[i]>=0){
			ans.push_back(table[i]);
			k=k - table[i];
			lol++;
		}
	}
	if(n==1){
		cout<<"1"<<endl;
		cout<<"1";
		return 0;
	}
	if(n==2){
		cout<<"1"<<endl;
		cout<<"2";
		return 0;
	}
	cout<<lol<<endl;
	for (int j=ans.size()-1;j>0-1;j--){
		cout<<ans[j]<<" ";
	}
	return 0;
}

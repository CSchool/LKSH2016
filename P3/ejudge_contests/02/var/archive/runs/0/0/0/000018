#include<iostream>

using namespace std;

int main()
    {
    long long z,s,s2,s3,s4,i,k,a,b,c,d,m[10000];
	cin>>a>>b>>c>>d>>k;
	if (a>c){
		swap(a,c);
		swap(b,d);}
	if (k==a) {
		cout<<b; return 0;}
	if (k==c) {
		cout<<d; return 0;} 
	s=b;
	s3=0;
	s2=0; s4=1;
	for (i=a;i<c;i++){
        z=s;
		s=s+s3;
        s3=z;
		z=s2;
		s2=s2+s4;
		s4=z;
	   }
	z=(d-s)/s2;
	m[a]=b;
	m[a-1]=z;
	if (k==a-1){
		cout<<z; return 0; }
	if (k<a){
		for (i=a-2;i>=k;i--){
		   m[i]=m[i+2]-m[i+1];
		   }
	   }
	if (k>a){
		for (i=a+1;i<=k;i++){
		   m[i]=m[i-1]+m[i-2];
		}
	  }
	cout<<m[k];
	return 0;
}
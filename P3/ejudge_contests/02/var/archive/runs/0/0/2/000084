#include <iostream>
#include <string>
//#include <conio.h>
using namespace std;

int main() {
	int m = 1, n = 1, l = 0, z = 0, g = 0, y = 0;
	cin >> l;
	int a[100000], b[10000];
	a[1] = 1;
	a[0] = 1;
	for (int i = 2; n <= l; i++)	
	{
		a[i] = n + m;
		n = a[i];
		m = a[i - 1];
		z = i;
	}
	for (int i = z; l != 0; i--)
	{
		if (l - a[i] >= 0)
		{
			l = l - a[i];
			g++;
			b[g] = a[i];
			//cout << b[g] << ' ';
		}
	}
	cout << g << endl;
	for (int c = g; c >= 1; c--)
	{
		cout << b[c] << ' ';
	}
	//_getch();
}
#include <iostream>
//#include <conio.h>
#include <vector>
#include <string>

using namespace std;

int main() {
	string s;
	vector<int> k;

	cin >> s;
	k.resize(s.size());

	k[0] = 1;

	for (int i = 1; i < s.size(); i++) {
		int a, b;
		a = k[i - 1];
		b = i - 2 >= 0 ? k[i - 2] : 1;
		k[i] += a;
		if ((s[i - 1]>'0') && (s[i - 1] < '4')) {
			k[i] += b;
			if ((s[i - 1] == '3') && (s[i] > '3'))
				k[i] -= b;
		}
	}

	cout << k[k.size() - 1];

	//_getch();
	return 0;
}
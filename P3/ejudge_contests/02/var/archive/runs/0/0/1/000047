#include<iostream>

using namespace std;

int main()
    {
    int z,s,s2,i,k,a,b,c,d,m[3100];
	cin>>a>>b>>c>>d>>k;
	if (a>c){
		swap(a,c);
		swap(b,d);}
	if (k==a) {
		cout<<b; return 0;}
	if (k==c) {
		cout<<d; return 0;}
	for (z=-20000000;z<=200000000;z++){
		s=b;
		s2=z;
		for (i=a+1;i<c;i++){
		  if ((a+1)%2==0)
		  if (i%2!=0)
			s=s+s2;
		   else
			   s2=s+s2;
		  else
            if (i%2==0)
			s=s+s2;
		   else
			   s2=s+s2;
		}
		if (s+s2==d) {
	m[a+1100]=b;
	m[a-1+1100]=z;
	if (k==a-1){
		cout<<z; return 0; }
	if (k<a){
		for (i=a-2;i>=k;i--){
		   m[i+1100]=m[i+2+1100]-m[i+1+1100];
		   }
	   }
	if (k>a){
		for (i=a+1;i<=k;i++){
		   m[i+1100]=m[i-1+1100]+m[i-2+1100];
		}
	  }
	cout<<m[k+1100];
	return 0;
	break;
		}}
}
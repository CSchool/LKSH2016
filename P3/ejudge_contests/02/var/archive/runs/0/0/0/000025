#include<iostream>
#include<string>

using namespace std;

int main()
{
	int m[1000],i,j,p,n;
	string s;
    cin>>s;
	n=s.length();
	m[0]=1;
	if ((s[0]>='1'&&s[0]<='2'||s[0]=='3'&&(s[1]>='1'&&s[1]<='3'))||(s[1]=='0'||s[0]=='0')&&!(s[1]=='0'&&s[0]=='0'))
		m[1]=2;
	else
		m[1]=1;
	for (i=2;i<=n;i++){
	if ((s[i-1]>='1'&&s[i-1]<='2'||s[i-1]=='3'&&(s[i]>='1'&&s[i]<='3'))||(s[i]=='0'||s[i-1]=='0')&&!(s[i]=='0'&&s[i-1]=='0'))
		m[i]=m[i-2]*2;
	else
		m[i]=m[i-1]; 
	   }
	cout << m[n];
	return 0;
}
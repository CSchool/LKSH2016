// perviy.cpp: определяет точку входа для консольного приложения.
//

#include"iostream"
#include"vector"
#include"algorithm"
#include"string"

using namespace std;



int main()
{
	//freopen("input.txt","r",stdin);
	//freopen("output.txt","w",stdout);
	string s;
	int ans=0;
	vector<char> alph(33);
	for(int i=0;i<33;i++){
		alph[i]=48+i;;
	}
	getline(cin,s);
	for (int i=0;i<s.size();i++){
		for(int j=0;j<33;j++){
			if((alph[j]==s[i]) || (alph[j]==s[i] && alph[j+1]==s[i+1])){
				ans++;
				break;
			}
		}
	}
	cout<<ans;
	return 0;
}

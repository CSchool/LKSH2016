#include<iostream>

using namespace std;

int main()
    {
    int z,s,s2,s3,s4,i,k,a,b,c,d,m[1000];
	cin>>a>>b>>c>>d>>k;
	if (a<c) s=b;
	else
	s=d; 
	s3=0;
	s2=0; s4=1;
	for (i=min(a,c);i<max(a,c);i++){
        z=s;
		s=s+s3;
        s3=z;
		z=s2;
		s2=s2+s4;
		s4=z;
	   }
	if (c>a)
	z=(d-s)/s2;
	if (a>c)
	z=(b-s)/s2;
	if (z==0){
		if (a>c)
			z=d-b;
	    if (c>a)z=b-d;
	}
	if (a<c)
	m[a]=b;
	else
	m[c]=d;
	m[min(a,c)-1]=z;
	if (k==min(a,c)-1){
		cout<<z; return 0; }
	if (k<min(a,c)){
		for (i=min(a,c)-2;i>=k;i--){
		   m[i]=m[i+2]-m[i+1];
		   }
	   }
	if (k>min(a,c)){
		for (i=min(a,c)+1;i<=k;i++){
		   m[i]=m[i-1]+m[i-2];
		}
	  }
	cout<<m[k];
	return 0;
}
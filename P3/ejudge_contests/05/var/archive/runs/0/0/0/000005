// zadacha.cpp: определяет точку входа для консольного приложения.
//

#include "iostream"
#include "vector"

using namespace std;

int main()
{
	//freopen("input.txt","r",stdin);
	//freopen("output.txt","w",stdout);
	int n;
	cin>>n;
	vector<int> mas(n+1);
	mas[0]=1;
	for(int i=1;i<=n;i++){
		if(i % 2==0)
			mas[i]=mas[i-1]+mas[i/2];
		else
			mas[i]=mas[i-1];
	}
	cout<<mas[n];
	return 0;
}


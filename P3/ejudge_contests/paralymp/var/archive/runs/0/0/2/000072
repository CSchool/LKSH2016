#include <iostream>
#include <vector>

using namespace std;

struct x {
	int number, value = 0;
};

vector<x> v;

int main() {
	
	int n;
	cin >> n;

	for (int i = 0; i < n; i += (n-i) / 100 + 1) {
		int k;
		cout << "? " << i << endl;
		cin >> k;
		for (int j = 0; j < v.size(); j++) {
			if (v[j].number == k) {
				v[j].value++;
				k = -1;
				break;
			}
		}
		if (k >= 0) {
			v.resize(v.size() + 1);
			v[v.size() - 1].number = k;
		}
	}

	int max = 0, k = 0;
	for (int j = 0; j < v.size(); j++) {
		if (v[j].value > max) {
			max = v[j].value;
			k = v[j].number;
		}
	}

	cout << "+ " << k << endl;

	//system("pause");
	return 0;
}
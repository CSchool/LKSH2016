#include<iostream>
#include<string>

using namespace std;

int d=0;

int main()
{   
    int mx=0,p=0,mxi=0,a=0,m[1000000000], n;
    cin >> n;
    for (int i=0;i<=1000000000;i++)
        m[i]=0;
    for (int i=1;i<=100;i++){
        cout << "? " << i << endl;
        cin >> a;
        m[a]++;
    }
    for (int i=0;i<=1000000000;i++){
        if (m[i]>mx){
            mx=m[i];
            mxi=i;
        }
    }
    cout<<mxi;
    return 0;
}
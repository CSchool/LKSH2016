#include "iostream"
#include "vector"
#include "string"
#include "stdio.h"
#include "algorithm"
#include "math.h"
using namespace std;


int main()
{
	//freopen("input.txt","r",stdin);
	//freopen("output.txt","w",stdout);
	int n,max=0;
    cin>>n;
    vector<char> a(n+1);
    vector<int> table(256);
    int z=1;
    if(n>200) {z=2; n=99;}
    else
        if(n>300) {z=3; n=99; }
    else
        if(n>500) {z=5; n=99;}
    else
        if(n>1000) {z=10; n=99;}
    int kek=1;
    for(int i=1;kek<100;i+=z){
        cout<<"? "<<i<<endl;
        cin>>a[i];
        table[a[i]]++;
        kek++;
    }
    int maxi=0;
    for(int i=0;i<256;i++){
        if(table[i]>max) { maxi=i-48; max=table[i];}
    }
    cout<<"+ "<<maxi;
	return 0;
}
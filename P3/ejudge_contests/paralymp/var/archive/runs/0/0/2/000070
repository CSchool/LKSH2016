#include "iostream"
#include "vector"
#include "string"
#include "stdio.h"
#include "algorithm"
#include "math.h"
using namespace std;


int main()
{
	freopen("input.txt","r",stdin);
	freopen("output.txt","w",stdout);
	int n,max=0;
    cin>>n;
    vector<char> a(n+2);
    vector<int> table(256);
    for(int i=1;i<=n;i+=1){
        cout<<"? "<<i<<endl;
        cin>>a[i];
        table[a[i]]++;
    }
    for(int i=0;i<256;i++){
        if(table[i]>max) max=table[i];
    }
    cout<<"+ "<<max;
    system("pause");
	return 0;
}
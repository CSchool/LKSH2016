#include "iostream"
#include "vector"
#include "string"
#include "stdio.h"
#include "algorithm"
#include "math.h"
#include "ctime"
#include "cstdlib"
using namespace std;


int main()
{
	//freopen("input.txt","r",stdin);
	//freopen("output.txt","w",stdout);
	int a,b,kek=0;
    cin>>a>>b;
    if(a==b){
        cout<<a+b;
        return 0;
    }
    else
        if(a>b){
            for(int i=0; i<b;i++)
                kek+=b;
        }
        else
            for(int i=0; i<a;i++){
                kek+=a;
            }
    cout<<kek;
	return 0;
}
#include <iostream>

using namespace std;

int main()
{
    char m[64];
    int black = 0, white = 0, red = 0, green = 0; 
    for(int i = 0; i < 64; i++)
    {
        cin >> m[i];
    }
    for(int i = 0; i < 64; i++)
    {
        switch (m[i])
        {
            case '0':
                black++;
                break;
            case '1':
                white++;
                break;
            case '2':
                red++;
                break;
            case '3':
                green++;
                break;
        }
    }
    if ( black == 0 || white == 0 || red == 0 || green == 0 )
    {
        cout << "BAD INPUT LIST" << endl;
        return 0;
    }
    for(int i = 0; i < 64; i++)
    {
        if( i == 7 || i == 15 || i == 23 || i == 31 || i == 39 || i == 47 || i == 55 || i == 63 )
        {
            if(m[i] == '2')
                cout << m[i] << endl;
            else
                cout << "-" << endl;
        }
        else
        { 
            if(m[i] == '2')
                cout << m[i];
            else
                cout << "-";
        }
    
    }
    cout << endl;
    cout << black << " " << white << " " << red << " " << green;
    return 0;
}
#include "iostream"
#include "vector"
#include "algorithm"    
#include "string"
using namespace std;
int main()
{
	//freopen("input.txt","r",stdin);
	//freopen("output.txt","w",stdout);
	long long n;
	cin>>n;
	vector<char> s(n+2);
	vector<int> f(n+2);
	for(int i=1;i<=n;i++){
		cin>>f[i]>>s[i];
	}
	for(int i=1;i<n+1;i++){
		for(int j=1+i;j<n+1;j++){
			if(f[j]>f[i]) {
				swap(f[i],f[j]);
				swap(s[i],s[j]);
			}
		}
	}
	long long r=0,g=0,b=0;
	for(int i=n;i>=1;i--){
		if(s[i]=='R'){
			if(i!=n)
				r+=f[i]*f[i]*5-f[i+1]*f[i+1];
			if(i==n)r+=f[i]*f[i]*5;
		}
		if(s[i]=='G'){
			if(i!=n)
				g+=f[i]*f[i]*5-f[i+1]*f[i+1];
			if(i==n) g+=f[i]*f[i]*5;
		}
		if(s[i]=='B'){
			if(i!=n)
				b+=f[i]*f[i]*5-f[i+1]*f[i+1];
			if(i==n) b+=f[i]*f[i]*5;
		}
	}
	cout<<"R - "<<r<<endl;;
	cout<<"G - "<<g<<endl;
	cout<<"B - "<<b;
	return 0;
}

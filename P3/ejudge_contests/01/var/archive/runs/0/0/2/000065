#include "iostream"
#include "vector"
#include "algorithm"    
#include "string"
using namespace std;
int main()
{
	string s;
	getline(cin,s);
	vector<int> mas(256);
	int min=32000,max=0;
	for(int i=0;i<s.size();i++){
		mas[s[i]]++;
	}
	for(int i=0;i<256;i++){
		if(mas[i]>max) max=mas[i];
		if(mas[i]<min && mas[i]!=0) min=mas[i];
	}
	cout<<max-min;
	//system("pause");
	return 0;
}

#include <iostream>

using namespace std;

int main() {
	int n(0), s(0), b(0), f(0), i(0);
	cin >> n >> s;
	for (b; b < n; b++) {
		i = i + 1;
		f = f + i;
	}
	for (int j(1); j <= n; j++)
	for (int i(1); i <= n; i++)
	if (f - i - j == s) {
		cout << "yes" << endl << i << " " << j;
		return 0;
	}
	cout << "no" << b;
}
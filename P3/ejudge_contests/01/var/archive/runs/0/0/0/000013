// perviy.cpp: определяет точку входа для консольного приложения.
//

#include"iostream"

using namespace std;

int main()
{
	char s[10][10];
	char table[10][10];
	int red=0, black=0, white=0, green=0;
	for(int i=0;i<8;i++){
		for(int j=0;j<8;j++){
			cin>>s[i][j];
			if (s[i][j]=='0') black++;
			if (s[i][j]=='1') white++;
			if (s[i][j]=='2') {red++;
				table[i][j]='2';
				continue;
			} 
			if(s[i][j]=='3') green++;
			table[i][j]='-';
		}
	}
	for(int i=0;i<8;i++){
		for(int j=0;j<8;j++){
			cout<<table[i][j];
		}
		cout<<endl;
	}
	cout<<endl;
	cout<<black<<" "<<white<<" " <<red<<" "<<green;
	return 0;
}


// zadacha.cpp: определяет точку входа для консольного приложения.
//

#include "iostream"
#include "vector"

int main()
{
	long long n,s,sum=0;
	cin>>n>>s;
	vector<long long> table(n);
	for(int i=0;i<=n;i++)
		sum+=i;
	for (int i=1;i<=n;i++){
		for(int j=1;j<=n;j++){
			if(i!=j && i+j+sum==s && sum!=s){
				cout<<"yes"<<endl;
				cout<<i<<" "<<j;
				return 0;
			}
		}		
	}
	cout<<"no";
	return 0;
}

